# Least common multiple in Pile programming language
import "std/io.pile"

proc Lcm
    over over > if
        over
    else
        dup
    end
    loop
        over over swap % 0 = if
            rot over over % 0 = if
                swap
                break
            end
            swap
        end
        1 +
    end
    rot rot drop drop
end

420 69 Lcm Print
